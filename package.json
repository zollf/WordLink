{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "test": "jest",
        "lint": "eslint . --ext .ts,.tsx",
        "fix": "eslint . --ext .ts,.tsx --fix"
    },
    "pre-commit": [
        "test",
        "lint"
